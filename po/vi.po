#: fortified.schemas.in.h:18
msgid "Filter out hits not targeted at the firewall"
msgstr "Lọc bỏ các cú quét không nhắm tới tường lửa"

#: fortified.schemas.in.h:19
msgid ""
"Filter out hits that have an destination IP that does not match the firewall "
"host."
msgstr "Lọc bỏ các cú quét có IP đích không tương ứng với máy tường lửa."

#: fortified.schemas.in.h:20
msgid "Filter out redundant hits"
msgstr "Lọc ra các cú quét thừa"

#: fortified.schemas.in.h:21
msgid "Filter out sequences of identical hits."
msgstr "Lọc ra các chuỗi cú quét giống nhau"

